{
  "name": "mochify-example",
  "version": "0.0.1",
  "description": "Mochify example",
  "main": "src/main.js",
  "scripts": {
    "start": "budo src/main.js --port 8081 --live --open -- -t [ babelify [ es2015 ] ]",
    "test":  "mochify --transform [ babelify [ es2015 ] ] --timeout 5000 --reporter spec src/**/*.test.js",
    "watch": "mochify --transform [ babelify [ es2015 ] ] --timeout 5000 --reporter spec --watch src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomastrajan/mochify-example.git"
  },
  "keywords": [
    "mochify",
    "testing",
    "node",
    "browser"
  ],
  "author": "Tomas Trajan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomastrajan/mochify-example/issues"
  },
  "homepage": "https://github.com/tomastrajan/mochify-example#readme",
  "devDependencies": {
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "mochify": "^2.14.2",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "bean": "^1.0.15",
    "bonzo": "^2.0.0",
    "lodash": "^3.10.1",
    "mustache": "^2.2.0"
  }
}
